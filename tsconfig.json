{
  "compilerOptions": {
    "jsx": "react-jsx",
    "outDir": "./dist/",
    "module": "es2020",
    "target": "es6",
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowUmdGlobalAccess": true,
    "paths": {
      "#types/*": ["./types/*"]
    }
  },
  "exclude": [
    "**/node_modules/**",
    "**/build/**",
    "**/dist/**",
    "**/target/**",
    "./api/**",
    "./common/**",
    "./deploy/**",
    "./docker/**",
    "./manager/node/**",
    "./manager/src/main/java/**",
    "./officialplugins/**",
    "./plugins/**"
  ]
}