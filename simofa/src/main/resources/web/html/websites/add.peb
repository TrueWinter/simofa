{% extends web_root + "fragments/template.peb" %}
{% block title %}Add Website{% endblock %}
{% block content %}
    <h1>Add Website</h1>
    {% if error is not empty %}
        <div class="error">{{ error }}</div>
    {% endif %}
    {% if success is not empty %}
        <div class="success">{{ success }}</div>
    {% endif %}

    <div id="app"></div>
{% endblock %}